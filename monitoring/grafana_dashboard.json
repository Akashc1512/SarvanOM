          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "SarvanOM Gateway Docs",
        "url": "https://docs.sarvanom.com",
        "type": "link"
      },
      {
        "title": "Metrics Endpoint",
        "url": "http://localhost:8000/metrics",
        "type": "link"
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "up == 0",
          "iconColor": "red",
          "enable": true,
          "showIn": 0
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "provider",
          "type": "query",
          "query": "label_values(provider_requests_total, provider)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "endpoint",
          "type": "query",
          "query": "label_values(http_requests_total, endpoint)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}
