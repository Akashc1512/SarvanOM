        "type": "stat",
        "targets": [
          {
            "expr": "rate(sse_heartbeats_total[5m])",
            "legendFormat": "Heartbeats/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
